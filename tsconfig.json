{
    "compilerOptions": {
      "strict": true,
      "moduleDetection": "auto",
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "module": "CommonJS",
      
      "moduleResolution": "node",
      "types": ["node"],
      "outDir": ".",
      "target": "ES2020",
    },"exclude": [".history","./dist"]
  }
  